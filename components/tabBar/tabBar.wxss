/* components/tabBar/tabBar.wxss */
.tabBar {
  position: fixed; bottom: 0; left: 0; right: 0; display: flex; height: 50px; border-top: 2rpx solid #ccc; color: #6e6d6b; background: #fff; z-index: 999;
}
.tabBarList {
  flex: 1; display: flex; flex-direction: column; justify-content: center; align-items: center; font-size: 20rpx;
}
.tabBarList .img {
  width: 60rpx; height: 60rpx;
}
.tabBarActive {
  color: #ff475e;
}